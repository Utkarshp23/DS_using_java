package merge_Lists;

import linkedlist.List;

public class Main {

	public static void mergeLL(List l1, List l2)
	{
		l1.tail.next=l2.head;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		List l1= new List();
		List l2= new List();
		
		l1.createLL(5);
		l2.createLL(5);
	}

}
